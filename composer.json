{
    "name": "alex-kalanis/kw_modules",
    "description": "Selecting correct modules in KWCMS",
    "keywords": [
        "module",
        "modules"
    ],
    "license": "BSD-3-Clause",
    "type": "library",
    "require": {
        "ext-ctype": "*",
        "ext-json": "*",
        "ext-mbstring": "*",
        "php": ">=7.2.0"
    },
    "require-dev": {
        "alex-kalanis/kw_input": ">=1.0",
        "alex-kalanis/kw_auth": ">=1.0",
        "alex-kalanis/kw_paths": ">=1.0",
        "alex-kalanis/kw_confs": ">=1.0",
        "alex-kalanis/kw_storage": ">=1.0",
        "alex-kalanis/kw_templates": ">=1.0",
        "alex-kalanis/kw_address_handler": ">=1.0",
        "phpunit/phpunit": ">=7.2"
    },
    "suggest": {
        "alex-kalanis/kw_autoload": "Autoloader",
        "alex-kalanis/kw_storage": "Local storage for accessing resources"
    },
    "autoload": {
        "psr-4": {
            "kalanis\\kw_modules\\": "php-src/"
        }
    },
    "scripts": {
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
