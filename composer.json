{
    "name": "alex-kalanis/kw_modules",
    "description": "Selecting correct modules in KWCMS",
    "keywords": [
        "module",
        "modules"
    ],
    "license": "BSD-3-Clause",
    "type": "library",
    "require": {
        "alex-kalanis/kw_input": ">=2.3 <3",
        "alex-kalanis/kw_paths": ">=3.1 <4",
        "ext-json": "*",
        "ext-mbstring": "*",
        "php": ">=7.2.0"
    },
    "require-dev": {
        "alex-kalanis/kw_files": ">=3.0 <4",
        "alex-kalanis/kw_storage": ">=4.0 <5",
        "psr/container": "=2.0.2",
        "friendsofphp/php-cs-fixer": "^3.0",
        "phpstan/phpstan": "^1.0",
        "phpstan/phpstan-phpunit": "^1.0",
        "phpunit/phpunit": ">=7.2 <=9"
    },
    "suggest": {
        "alex-kalanis/kw_autoload": "Autoloader",
        "alex-kalanis/kw_storage": "Local storage for accessing resources"
    },
    "autoload": {
        "psr-4": {
            "kalanis\\kw_modules\\": "php-src/"
        }
    },
    "scripts": {
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
